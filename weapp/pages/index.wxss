.container {
  background: #f9f1da;
  height: 100%;
}
.content {
  width: 100%;
  height: 300rpx;
  text-align: center;
  font-size: 60rpx;
  line-height: 300rpx;
  background: rgba(246, 200, 60, 0.5);
}
.btn {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 100rpx auto;
  background: rgba(246, 200, 60);
  border: 4rpx solid #fff;
  border-radius: 8rpx;
}

